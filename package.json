{
    "name": "Finish-em",
    "version": "0.4.10",
    "author": "Ryan Scott (ryan@ryankennethscott.com)",
    "description": "An org-mode inspired personal organiser",
    "repository": {
        "type": "git",
        "url": "git://github.com/ryankscott/finish-em/"
    },
    "main": "main.js",
    "homepage": "./",
    "build": {
        "appId": "com.ryankscott.finish-em",
        "files": [
            "main.js",
            "preload.js",
            "src/assets/*",
            "build/**/*"
        ],
        "publish": {
            "provider": "github"
        }
    },
    "scripts": {
        "dev": "parcel ./src/index.html",
        "start-dev": "ELECTRON_START_URL=http://localhost:1234 electron .",
        "start": "electron .",
        "clean": "rm -rf build/ .cache dist/",
        "build": "parcel build ./src/index.html --no-minify -d build/ --public-url ./",
        "pack": "electron-builder --dir",
        "dist": "electron-builder -m -p onTag",
        "postinstall": "electron-builder install-app-deps",
        "test": "jest",
        "test:watch": "npm test -- --watch",
        "deploy": "electron-builder build --mac --publish always"
    },
    "devDependencies": {
        "@babel/core": "^7.11.0",
        "@babel/preset-env": "^7.11.0",
        "@babel/preset-typescript": "^7.10.4",
        "@typed/uuid": "^9.7.0",
        "@types/css": "0.0.31",
        "@types/jest": "^26.0.8",
        "@types/react": "^16.9.44",
        "@types/react-beautiful-dnd": "^13.0.0",
        "@types/react-day-picker": "^5.3.0",
        "@types/react-dom": "^16.9.8",
        "@types/react-redux": "^7.1.9",
        "@types/react-router-dom": "^5.1.5",
        "@types/react-select": "^3.0.15",
        "@types/react-tooltip": "^4.2.4",
        "@types/styled-components": "^5.1.2",
        "@typescript-eslint/eslint-plugin": "^3.7.1",
        "@typescript-eslint/parser": "^3.7.1",
        "autoprefixer": "^9.8.6",
        "babel-jest": "^26.2.2",
        "babel-plugin-styled-components": "^1.11.1",
        "cypress": "^4.11.0",
        "electron": "9.1.2",
        "electron-builder": "^22.8.0",
        "electron-devtools-installer": "^3.1.1",
        "electron-reload": "^1.5.0",
        "eslint": "^7.6.0",
        "eslint-config-airbnb": "^18.2.0",
        "eslint-plugin-import": "^2.22.0",
        "eslint-plugin-jsx-a11y": "^6.3.1",
        "eslint-plugin-react": "^7.20.5",
        "eslint-plugin-react-hooks": "^4.0.8",
        "jest": "^26.2.2",
        "mockdate": "^3.0.2",
        "parcel": "^1.12.4",
        "parcel-bundler": "^1.12.4",
        "postcss-modules": "^3.2.0",
        "typescript": "^3.9.7"
    },
    "dependencies": {
        "@babel/runtime": "^7.11.0",
        "@types/marked": "^1.1.0",
        "applescript": "^1.0.0",
        "chrono-node": "^2.1.5",
        "csstype": "^3.0.2",
        "date-fns": "^2.15.0",
        "electron-is-dev": "^1.2.0",
        "electron-store": "^6.0.0",
        "emoji-regex": "^9.0.0",
        "eslint-config-prettier": "^6.11.0",
        "eslint-plugin-prettier": "^3.1.4",
        "filtrex": "^2.0.0",
        "immer": "^7.0.7",
        "is-electron": "^2.2.0",
        "lodash": "^4.17.19",
        "marked": "^1.1.1",
        "parcel-plugin-markdown": "^0.4.2",
        "parser.js": "^1.0.0",
        "polished": "^3.6.5",
        "prettier": "^2.0.5",
        "randomcolor": "^0.6.2",
        "react": "^16.13.1",
        "react-animations": "^1.0.0",
        "react-beautiful-dnd": "^13.0.0",
        "react-color": "^2.18.1",
        "react-day-picker": "^7.4.8",
        "react-dom": "^16.13.1",
        "react-highlight-words": "^0.16.0",
        "react-hotkeys": "^2.0.0",
        "react-hotkeys-hook": "^2.2.2",
        "react-redux": "^7.2.1",
        "react-router": "^5.2.0",
        "react-router-dom": "^5.2.0",
        "react-select": "^3.1.0",
        "react-switch": "^5.0.1",
        "react-toastify": "^6.0.8",
        "react-tooltip": "^4.2.7",
        "redux": "^4.0.5",
        "redux-persist": "^6.0.0",
        "redux-persist-electron-storage": "^2.1.0",
        "reselect": "^4.0.0",
