{
    "name": "Finish-em",
    "version": "0.3.15",
    "author": "Ryan Scott (ryan@ryankennethscott.com)",
    "description": "An org-mode inspired personal organiser",
    "repository": {
        "type": "git",
        "url": "git://github.com/ryankscott/finish-em/"
    },
    "main": "main.js",
    "homepage": "./",
    "build": {
        "appId": "com.ryankscott.finish-em",
        "files": [
            "main.js",
            "preload.js",
            "src/assets/*",
            "build/**/*"
        ],
        "publish": {
            "provider": "github"
        }
    },
    "scripts": {
        "dev": "parcel ./src/index.html",
        "start-dev": "ELECTRON_START_URL=http://localhost:1234 electron .",
        "start": "electron .",
        "clean": "rm -rf build/ .cache dist/",
        "build": "parcel build ./src/index.html --no-minify -d build/ --public-url ./",
        "pack": "electron-builder --dir",
        "dist": "electron-builder -m -p onTag",
        "postinstall": "electron-builder install-app-deps",
        "test": "jest",
        "test:watch": "npm test -- --watch",
        "deploy": "electron-builder build --mac --publish always"
    },
    "devDependencies": {
        "@babel/core": "^7.10.4",
        "@babel/preset-env": "^7.10.4",
        "@babel/preset-typescript": "^7.10.4",
        "@typed/uuid": "^8.2.0",
        "@types/css": "0.0.31",
        "@types/jest": "^25.2.2",
        "@types/react": "^16.9.41",
        "@types/react-beautiful-dnd": "^12.1.3",
        "@types/react-day-picker": "^5.3.0",
        "@types/react-dom": "^16.9.8",
        "@types/react-redux": "^7.1.9",
        "@types/react-router-dom": "^5.1.5",
        "@types/react-select": "^3.0.14",
        "@types/react-tooltip": "^3.11.0",
        "@types/styled-components": "^5.1.1",
        "@typescript-eslint/eslint-plugin": "^2.34.0",
        "@typescript-eslint/parser": "^2.34.0",
        "autoprefixer": "^9.8.4",
        "babel-jest": "^25.5.1",
        "babel-plugin-styled-components": "^1.10.7",
        "cypress": "^4.9.0",
        "electron": "7.3.0",
        "electron-builder": "^22.7.0",
        "electron-devtools-installer": "^2.2.4",
        "electron-reload": "^1.5.0",
        "eslint": "^6.8.0",
        "eslint-config-airbnb": "^18.2.0",
        "eslint-plugin-import": "^2.22.0",
        "eslint-plugin-jsx-a11y": "^6.3.1",
        "eslint-plugin-react": "^7.20.3",
        "eslint-plugin-react-hooks": "^2.5.1",
        "jest": "^25.5.4",
        "mockdate": "^2.0.5",
        "parcel": "^1.12.4",
        "parcel-bundler": "^1.12.4",
        "postcss-modules": "^1.5.0",
        "typescript": "^3.9.6"
    },
    "dependencies": {
        "@babel/runtime": "^7.10.4",
        "@m93a/filtrex": "^2.1.0-beta3",
        "@types/marked": "^0.7.4",
        "applescript": "^1.0.0",
        "chrono-node": "^1.4.6",
        "csstype": "^2.6.11",
        "date-fns": "^2.14.0",
        "electron-is-dev": "^1.2.0",
        "electron-store": "^5.2.0",
        "emoji-regex": "^9.0.0",
        "eslint-config-prettier": "^6.11.0",
        "eslint-plugin-prettier": "^3.1.4",
        "filtrex": "^2.0.0",
        "immer": "^6.0.9",
        "is-electron": "^2.2.0",
        "lodash": "^4.17.15",
        "marked": "^0.8.2",
        "parcel-plugin-markdown": "^0.4.2",
        "parser.js": "^1.0.0",
        "polished": "^3.6.5",
        "prettier": "^2.0.5",
        "react": "^16.13.1",
        "react-animations": "^1.0.0",
        "react-beautiful-dnd": "^13.0.0",
        "react-color": "^2.18.1",
        "react-day-picker": "^7.4.8",
        "react-dom": "^16.13.1",
        "react-hotkeys": "^2.0.0",
        "react-redux": "^7.2.0",
        "react-router": "^5.2.0",
        "react-router-dom": "^5.2.0",
        "react-select": "^3.1.0",
        "react-switch": "^5.0.1",
        "react-toastify": "^6.0.8",
        "react-tooltip": "^4.2.7",
        "redux": "^4.0.5",
        "redux-persist": "^6.0.0",
        "redux-persist-electron-storage": "^2.1.0",
        "reselect": "^4.0.0",
        "rrule": "^2.6.4",
        "styled-components": "^4.4.1",
        "uuid": "^3.4.0"
    }
}
