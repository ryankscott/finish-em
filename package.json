{
  "name": "Finish-em",
  "version": "0.6.2",
  "author": "Ryan Scott (ryan@ryankennethscott.com)",
  "description": "An org-mode inspired personal organiser",
  "repository": {
    "type": "git",
    "url": "git://github.com/ryankscott/finish-em/"
  },
  "main": "build/main/main.js",
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "start": "run-s main:build electron:watch",
    "test": "jest",
    "clean": "rm -rf build/",
    "build": "run-s clean parcel:build copyicon electron:build",
    "copyicon": "cp app/renderer/assets/finish_em.png build/icon.png",
    "release": "run-s parcel:build \"electron:build {@}\" --",
    "electron:watch": "run-p -r parcel:watch electron:start",
    "electron:start": "APP_DEV=true nodemon --watch  /build/app/main --exec \"electron .\"",
    "electron:build": "electron-builder --x64 --publish=never",
    "parcel:watch": "run-p -r main:watch renderer:watch",
    "main:watch": "parcel watch app/main/main.ts --out-dir build/main/ --out-file main --target electron",
    "renderer:watch": "parcel app/renderer/index.html --port 1124 --out-dir build/renderer/development",
    "parcel:build": "run-s main:build renderer:build",
    "main:build": "parcel build app/main/main.ts --out-dir build/main/ --out-file main --target electron",
    "renderer:build": "parcel build app/renderer/index.html --public-url ./ --out-dir build/renderer/production",
    "dist": "electron-builder -m -p onTag",
    "deploy": "electron-builder build --mac --publish always",
    "generate": "graphql-codegen --config codegen.yml"
  },
  "license": "MIT",
  "browserslist": [
    "last 2 Chrome versions"
  ],
  "build": {
    "productName": "Finish-em",
    "appId": "com.ryankscott.finish-em",
    "files": [
      "build/main/**",
      "build/renderer/production/**"
    ],
    "mac": {
      "target": [
        "dmg"
      ],
      "artifactName": "${productName}.${ext}"
    },
    "publish": {
      "provider": "github"
    },
    "directories": {
      "output": "release"
    },
    "extraResources": [
      {
        "from": "./app/main/migrations/",
        "to": "./resources/",
        "filter": [
          "**/*"
        ]
      }
    ]
  },
  "jest": {
    "verbose": true,
    "collectCoverageFrom": [
      "app/renderer/**",
      "!app/renderer/index.tsx",
      "!app/renderer/tests/**",
      "!app/renderer/.parcel/**"
    ],
    "transform": {
      "^.+\\.tsx?$": "babel-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.12.17",
    "@babel/preset-env": "^7.12.17",
    "@babel/preset-react": "^7.12.13",
    "@babel/preset-typescript": "^7.12.17",
    "@electron/typescript-definitions": "^8.8.0",
    "@graphql-codegen/cli": "1.19.4",
    "@graphql-codegen/introspection": "1.18.1",
    "@graphql-codegen/typescript": "1.19.0",
    "@graphql-codegen/typescript-apollo-client-helpers": "^1.1.2",
    "@graphql-codegen/typescript-operations": "^1.17.14",
    "@graphql-codegen/typescript-react-apollo": "^2.2.1",
    "@graphql-codegen/typescript-resolvers": "^1.18.2",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "babel-jest": "^26.6.3",
    "electron": "^11.3.0",
    "electron-builder": "^22.9.1",
    "immer": "^8.0.1",
    "jest": "^26.6.3",
    "mockdate": "^3.0.2",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-pegjs": "0.0.2",
    "parcel-plugin-text": "^0.9.7",
    "pegjs": "^0.11.0-master.b7b87ea",
    "prettier": "^2.2.1",
    "react-test-renderer": "^17.0.1",
    "spectron": "^13.0.0",
    "tslint": "^6.1.1",
    "tslint-react": "^5.0.0",
    "typescript": "^4.1.5"
  },
  "dependencies": {
    "@apollo/client": "^3.3.11",
    "@babel/runtime": "^7.12.18",
    "@graphql-tools/merge": "^6.2.9",
    "@graphql-tools/schema": "^7.1.3",
    "@parcel/transformer-graphql": "^2.0.0-alpha.3",
    "@types/codemirror": "0.0.108",
    "@types/emoji-regex": "^8.0.0",
    "@types/lodash": "^4.14.168",
    "@types/pegjs": "^0.10.2",
    "@types/react-select": "^3.1.2",
    "apollo-link": "^1.2.14",
    "apollo-link-retry": "^2.2.16",
    "app-root-path": "^3.0.0",
    "applescript": "^1.0.0",
    "chrono-node": "^2.2.1",
    "codemirror": "^5.59.3",
    "cross-fetch": "^3.0.6",
    "csstype": "^3.0.6",
    "date-fns": "^2.17.0",
    "electron-log": "^4.3.2",
    "emoji-regex": "^9.2.1",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "express": "^4.17.1",
    "express-graphql": "^0.12.0",
    "express-jwt": "^6.0.0",
    "graphql": "^15.5.0",
    "graphql-scalars": "^1.7.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "marked": "^1.2.9",
    "morgan": "^1.10.0",
    "node": "^15.8.0",
    "node-cron": "^2.0.3",
    "parcel-plugin-markdown": "^0.4.2",
    "polished": "^4.1.1",
    "quill-markdown-shortcuts": "0.0.10",
    "react": "^17.0.1",
    "react-animations": "^1.0.0",
    "react-beautiful-dnd": "^13.0.0",
    "react-codemirror2": "^7.2.1",
    "react-colorful": "^4.4.4",
    "react-command-palette": "^0.13.1-0",
    "react-datepicker": "^3.5.0",
    "react-dom": "^17.0.1",
    "react-hotkeys-hook": "^2.4.1",
    "react-quill": "^1.3.5",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-select": "^3.2.0",
    "react-spinners": "^0.9.0",
    "react-switch": "^5.0.1",
    "react-toastify": "^6.2.0",
    "react-tooltip": "^4.2.14",
    "react-transition-group": "^4.4.1",
    "rrule": "2.6.6",
    "semver": "^7.3.4",
    "sql-template-strings": "^2.2.2",
    "sqlite": "^4.0.19",
    "sqlite3": "^5.0.2",
    "sqlstring-sqlite": "^0.1.1",
    "styled-components": "^5.2.1",
    "sugar-date": "^2.0.6",
    "uuid": "^8.3.2"
  }
}
