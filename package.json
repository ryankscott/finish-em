{
  "name": "Finish-em",
  "version": "0.8.19",
  "description": "An org-mode inspired personal organiser",
  "main": "./out/main/index.js",
  "author": {
    "name": "Ryan Scott",
    "email": "ryan@ryankennethscott.com"
  },
  "homepage": "https://github.com/ryankscott/finish-em/readme",
  "repository": {
    "type": "git",
    "url": "git://github.com/ryankscott/finish-em/"
  },
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "build": "npm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:unpack": "npm run build && electron-builder --dir",
    "build:win": "npm run build && electron-builder --win",
    "build:mac": "electron-vite build && electron-builder --mac",
    "build:linux": "electron-vite build && electron-builder --linux"
  },
  "dependencies": {
    "@apollo/client": "^3.11.4",
    "@apollo/server": "^4.11.0",
    "@chakra-ui/react": "^2.8.2",
    "@electron-toolkit/preload": "^3.0.1",
    "@electron-toolkit/utils": "^3.0.0",
    "@emoji-mart/data": "^1.2.1",
    "@emoji-mart/react": "^1.1.1",
    "@emotion/react": "^11.13.0",
    "@emotion/styled": "^11.13.0",
    "@graphql-tools/load-files": "^7.0.0",
    "chrono-node": "^1.4.9",
    "colormap": "^2.3.2",
    "datasource-sql": "^2.1.0",
    "date-fns": "^3.6.0",
    "date-fns-tz": "^3.1.3",
    "electron-log": "^5.1.7",
    "electron-store": "^10.0.0",
    "electron-updater": "^6.1.7",
    "framer-motion": "^11.3.28",
    "graphql-scalars": "^1.23.0",
    "marked": "^14.0.0",
    "node-fetch": "^3.3.2",
    "open": "^10.1.0",
    "polished": "^4.3.1",
    "quill-markdown-shortcuts": "^0.0.10",
    "react-beautiful-dnd": "^13.1.1",
    "react-colorful": "^5.6.1",
    "react-command-palette": "^0.22.1",
    "react-datepicker": "^7.3.0",
    "react-icons": "^5.3.0",
    "react-querybuilder": "^7.6.1",
    "react-quill": "^2.0.0",
    "react-router-dom": "^6.26.1",
    "react-select": "^5.8.0",
    "react-toastify": "^10.0.5",
    "rrule": "^2.8.1",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7",
    "use-debounce": "^10.0.2",
    "uuid": "^10.0.0",
    "xcall": "^1.0.4",
    "zustand": "^4.5.5"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config-prettier": "^2.0.0",
    "@electron-toolkit/eslint-config-ts": "^2.0.0",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@types/node": "^20.14.8",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "electron": "^31.0.2",
    "electron-builder": "^24.13.3",
    "electron-vite": "^2.3.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.3",
    "prettier": "^3.3.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "typescript": "^5.5.2",
    "vite": "^5.3.1"
  }
}
